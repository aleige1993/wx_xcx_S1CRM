/* pages/modifyPrice/historyInfo/index.wxss */
.container {
  font-size: 24rpx;
  height: 100%;
  background: #fff;
  text-align: center;
}
.list {
  
}
.list-container {
  background-color: rgb(243, 243, 243);
  border-color: rgb(243, 243, 243);
  padding: 20rpx;
}

.list-container .wux-col{
  padding: 10rpx 0;
}

.container .time{
  text-align: center;
  margin: 40rpx 30rpx;
}
.container .time>text{
  padding: 10rpx 20rpx;
  color: #fff;
  background: #707070;
  border-radius: 24rpx;
}

.container .title{
  color: #000;
}

.container .content{
  color: #707070;
}

.container .content.name{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}